{
	"name": "aws-cdk-get-release-action",
	"version": "0.0.0",
	"description": "GitHub Action that retrieves the Node.js packages from an AWS CDK release by tag",
	"main": "src/index.ts",
	"author": "C. Spencer Beggs <spencer@beg.gs>",
	"repository": {
		"url": "https://github.com/spencerbeggs/aws-cdk-get-release-action.git",
		"type": "git"
	},
	"license": "MIT",
	"scripts": {
		"action": "yarn build && act",
		"clean": "rm -rf dist",
		"gql:fetch-schema": "graphql get-schema --project prod",
		"test": "NODE_ENV=staging jest",
		"bootstrap": "ts-node lib/bootstrap.ts init",
		"build": "yarn clean && yarn rollup && yarn package",
		"lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx,.json}' --quiet --fix --ignore-path .gitignore",
		"package": "ts-node --project tsconfig.run.json lib/package.ts",
		"start": "babel-node -x \".ts\" src/test.ts",
		"type-check": "tsc -p build.json --noEmit",
		"dev": "APP_ENV=local rollup -c rollup.config.ts --watch",
		"rollup": "rollup -c rollup.config.ts"
	},
	"devDependencies": {
		"@babel/cli": "^7.10.3",
		"@babel/core": "^7.10.3",
		"@babel/node": "^7.10.3",
		"@babel/plugin-proposal-class-properties": "^7.10.1",
		"@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.1",
		"@babel/plugin-proposal-object-rest-spread": "^7.10.3",
		"@babel/plugin-proposal-optional-chaining": "^7.10.3",
		"@babel/preset-env": "^7.10.3",
		"@babel/preset-typescript": "^7.10.1",
		"@octokit/graphql-schema": "^8.13.0",
		"@playlyfe/gql": "^2.6.2",
		"@rollup/plugin-babel": "^5.0.4",
		"@rollup/plugin-commonjs": "^13.0.0",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^8.1.0",
		"@rollup/plugin-typescript": "^5.0.0",
		"@types/bent": "^7.0.2",
		"@types/child-process-promise": "^2.2.1",
		"@types/fs-extra": "^9.0.1",
		"@types/inquirer": "^6.5.0",
		"@types/jest": "^26.0.0",
		"@types/lodash": "^4.14.156",
		"@types/node": "^14.0.6",
		"@types/prettier": "^2.0.1",
		"@types/yargs": "^15.0.5",
		"@typescript-eslint/eslint-plugin": "^3.4.0",
		"@typescript-eslint/parser": "^3.4.0",
		"awesome-typescript-loader": "^5.2.1",
		"babel-eslint": "^10.1.0",
		"babel-jest": "^26.1.0",
		"babel-loader": "^8.1.0",
		"babel-plugin-lodash": "^3.3.4",
		"babel-plugin-macros": "^2.8.0",
		"babel-plugin-module-resolver": "^4.0.0",
		"bent": "^7.3.6",
		"chalk": "^4.1.0",
		"change-case": "^4.1.1",
		"child-process-promise": "^2.2.1",
		"dev-console.macro": "^0.1.0",
		"dotenv": "^8.2.0",
		"eslint": "^7.3.1",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-jest": "^23.17.1",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
		"fs-extra": "^9.0.0",
		"graphql": "^15.1.0",
		"graphql-cli": "^4.0.0",
		"graphql-config": "^3.0.2",
		"graphql-tag": "^2.10.3",
		"graphql-tag.macro": "^2.0.1",
		"husky": "^4.2.5",
		"inquirer": "^7.2.0",
		"jest": "^26.1.0",
		"jest-transform-css": "^2.0.0",
		"lint-staged": "^10.2.11",
		"lodash": "^4.17.15",
		"postcss": "^7.0.32",
		"prettier": "^2.0.5",
		"rollup": "^2.18.0",
		"rollup-plugin-dts": "^1.4.7",
		"source-map-support": "^0.5.19",
		"ts-babel-node": "^1.1.1",
		"ts-jest": "^26.1.1",
		"ts-loader": "^7.0.5",
		"ts-node": "^8.10.2",
		"tsconfig-paths": "^3.9.0",
		"tslib": "^2.0.0",
		"typescript": "^3.9.3",
		"yargs": "^15.3.1"
	},
	"dependencies": {
		"@actions/core": "^1.2.4",
		"@actions/github": "^3.0.0",
		"@octokit/graphql": "^4.5.1"
	}
}
