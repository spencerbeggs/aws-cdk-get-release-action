{
	"name": "aws-cdk-get-release-action",
	"version": "0.0.0",
	"description": "GitHub Action that retrieves the Node.js packages from an AWS CDK release by tag",
	"main": "src/index.ts",
	"author": "C. Spencer Beggs <spencer@beg.gs>",
	"repository": {
		"url": "https://github.com/spencerbeggs/aws-cdk-get-release-action.git",
		"type": "git"
	},
	"license": "MIT",
	"scripts": {
		"action": "yarn build && act",
		"clean": "rm -rf dist",
		"debug": "node --inspect dist/index.js",
		"gql:fetch-schema": "graphql get-schema --project prod",
		"test": "NODE_ENV=production APP_ENV=jest jest --setupFiles dotenv/config",
		"bootstrap": "ts-node lib/bootstrap.ts init",
		"build": "yarn clean && NODE_ENV=production yarn rollup && yarn package && cd dist && yarn install --production=true --pure-lockfile",
		"lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx,.json}' --quiet --fix --ignore-path .eslintignore",
		"package": "ts-node --project tsconfig.run.json lib/package.ts",
		"start": "babel-node -x \".ts\" src/test.ts",
		"type-check": "tsc -p build.json --noEmit",
		"dev": "yarn clean && APP_ENV=local rollup -c rollup.config.ts --watch",
		"rollup": "rollup -c rollup.config.ts",
		"codegen": "graphql-codegen --config codegen.yml"
	},
	"devDependencies": {
		"@babel/cli": "^7.10.5",
		"@babel/core": "^7.10.5",
		"@babel/node": "^7.10.5",
		"@babel/plugin-proposal-class-properties": "^7.10.4",
		"@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
		"@babel/plugin-proposal-object-rest-spread": "^7.10.4",
		"@babel/plugin-proposal-optional-chaining": "^7.10.4",
		"@babel/preset-env": "^7.10.4",
		"@babel/preset-typescript": "^7.10.4",
		"@graphql-codegen/cli": "1.17.4",
		"@graphql-codegen/typescript": "1.17.4",
		"@graphql-codegen/typescript-document-nodes": "^1.17.4",
		"@graphql-codegen/typescript-operations": "^1.17.4",
		"@graphql-codegen/typescript-resolvers": "1.17.4",
		"@octokit/graphql-schema": "^8.18.0",
		"@playlyfe/gql": "^2.6.2",
		"@rollup/plugin-babel": "^5.1.0",
		"@rollup/plugin-commonjs": "^14.0.0",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^8.4.0",
		"@rollup/plugin-typescript": "^5.0.2",
		"@types/bent": "^7.3.0",
		"@types/child-process-promise": "^2.2.1",
		"@types/fs-extra": "^9.0.1",
		"@types/inquirer": "^7.3.0",
		"@types/jest": "^26.0.7",
		"@types/lodash": "^4.14.158",
		"@types/node": "^14.0.26",
		"@types/prettier": "^2.0.2",
		"@types/yargs": "^15.0.5",
		"@typescript-eslint/eslint-plugin": "^3.7.0",
		"@typescript-eslint/parser": "^3.7.0",
		"babel-eslint": "^10.1.0",
		"babel-jest": "^26.1.0",
		"babel-plugin-lodash": "^3.3.4",
		"babel-plugin-macros": "^2.8.0",
		"bent": "^7.3.7",
		"builtin-modules": "^3.1.0",
		"chalk": "^4.1.0",
		"change-case": "^4.1.1",
		"child-process-promise": "^2.2.1",
		"dev-console.macro": "^0.1.0",
		"dotenv": "^8.2.0",
		"eslint": "^7.5.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-jest": "^23.18.0",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
		"fs-extra": "^9.0.0",
		"graphql": "^15.3.0",
		"graphql-cli": "^4.0.0",
		"graphql-config": "^3.0.3",
		"graphql-tag": "^2.10.4",
		"graphql-tag.macro": "^2.0.1",
		"husky": "^4.2.5",
		"inquirer": "^7.3.3",
		"jest": "^26.1.0",
		"lint-staged": "^10.2.11",
		"lodash": "^4.17.19",
		"prettier": "^2.0.5",
		"rollup": "^2.23.0",
		"rollup-plugin-dts": "^1.4.9",
		"source-map-support": "^0.5.19",
		"ts-babel-node": "^1.1.1",
		"ts-jest": "^26.1.3",
		"ts-loader": "^7.0.5",
		"ts-node": "^8.10.2",
		"tsconfig-paths": "^3.9.0",
		"tslib": "^2.0.0",
		"typescript": "^3.9.7",
		"yargs": "^15.4.1"
	},
	"dependencies": {
		"@actions/core": "^1.2.4",
		"@actions/github": "^4.0.0",
		"@octokit/graphql": "^4.5.2",
		"apollo-cache-inmemory": "^1.6.6",
		"apollo-client": "^2.6.10",
		"apollo-link-http": "^1.5.17",
		"cross-fetch": "^3.0.5",
		"semver": "^7.3.2"
	}
}
